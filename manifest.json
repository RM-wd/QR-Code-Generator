{
  "manifest_version": 2,
  "name": "QR Code Generator",
  "version": "1.0",
  "description": "Generate QR code for the current webpage",
  "permissions": [
    "activeTab",
    "https://api.qrserver.com/"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}